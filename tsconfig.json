{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "outDir": "dist",
    "noImplicitThis": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "lib": ["esnext", "DOM", "DOM.Iterable"],
    "types": ["unplugin-icons/types/react", "react", "@stylex-extend/react"],
    "resolveJsonModule": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": true
  }
}
